set number "relativenumber

set expandtab
set tabstop=4
set softtabstop=4
set smarttab
set shiftwidth=4

"set foldcolumn=1

" Enable 256 colors with syntax highlight
" commented rows break syntax on st
set t_AB=[48;5;%dm
set t_AF=[38;5;%dm
"set t_8f=[38;2:%lu;%lu;%lum
"set t_8b=[48;2;%lu;%lu;%lum
colorscheme spectro
set t_Co=256
set termguicolors
syntax on

set ruler

set cursorline
set cursorcolumn

"autoindent
set ai
set cin

set showcmd
"set wildmenu
